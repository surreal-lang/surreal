// User model with JSON serialization

/// A user with an ID and name.
#[derive(to_json_derive)]
pub struct User {
    id: int,
    name: string,
}

impl User {
    /// Create a new user with the given ID and name.
    pub fn new(id: int, name: string) -> User {
        User { id: id, name: name }
    }
}
